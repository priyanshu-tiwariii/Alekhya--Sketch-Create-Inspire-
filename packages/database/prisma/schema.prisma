
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id        String   @unique
  createdAt DateTime @default(now())
  email     String   @unique
  name      String?
  userName  String   @id
  bio       String?  
  profilePhoto String? 
  provider String
}

